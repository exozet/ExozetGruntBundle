{
    "name": "exozet/grunt-bundle",
    "type": "symfony-bundle",
    "description": "Run npm, bower and grunt on cache warmup in symfony2.",
    "license": "MIT",
    "minimum-stability": "dev",
    "authors": [
        {
            "name": "GruntBundle Authors",
            "homepage": "https://github.com/exozet/ExozetGruntBundle/graphs/contributors"
        }
    ],
    "autoload":     {
        "psr-4": { "Exozet\\GruntBundle\\": "" }
    },
    "target-dir": "Exozet/GruntBundle",
    "config": {
        "bin-dir": "bin"
    },
    "require": {
        "php": ">=5.3.0"
    },
    "require-dev": {
        "squizlabs/php_codesniffer": "~1.0",
        "phpunit/phpunit": "~4.3",
        "codeclimate/php-test-reporter": "~0.1"
    }
}
